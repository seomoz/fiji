@namespace("com.moz.fiji.mapreduce.avro.generated")
protocol JobHistoryTableRecords {

  /** Record that encapsulates a Job History Table entry. */
  record JobHistoryEntry {
    /** Job ID. */
    string job_id;

    /** Job name. */
    string job_name;

    /** Job start time. */
    long job_start_time;

    /** Job end time. */
    long job_end_time;

    /** Job end status. */
    string job_end_status;

    /** Job counters. */
    string job_counters;

    /** Job configuration. */
    string job_configuration;

    /** Derived information. */
    map<string> extended_info = {};

    /** Individual counters. */
    map<long> counters_family = {};
  }
}
